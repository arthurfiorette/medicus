{
  "name": "medicus",
  "version": "0.0.2",
  "bugs": {
    "url": "https://github.com/arthurfiorette/medicus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/arthurfiorette/medicus.git"
  },
  "license": "MIT",
  "author": "Arthur Fiorette <me@arthur.place>",
  "sideEffects": false,
  "type": "commonjs",
  "main": "dist/index.js",
  "files": ["dist", "fastify.d.ts", "fastify.js", "LICENSE", "README.md"],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "prepare": "husky",
    "test": "c8 --reporter lcov --reporter text tsx --enable-source-maps --test test/**/*.test.ts"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.33.22",
    "close-with-grace": "^2.1.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@arthurfiorette/biomejs-config": "^1.0.5",
    "@biomejs/biome": "^1.9.4",
    "@fastify/under-pressure": "^9.0.1",
    "@types/node": "^22.9.0",
    "c8": "^10.1.2",
    "husky": "^9.1.6",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    },
    "fastify-plugin": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.1.1",
  "engines": {
    "node": ">=18"
  }
}
